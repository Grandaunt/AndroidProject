<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical"
    android:layout_marginTop="10dp"
    android:paddingBottom="@dimen/activity_horizontal_margin">
    <View
        android:layout_height="0.5dp"
        android:background="@color/gray_e7e7e7"
        android:layout_width="match_parent" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin">

    <TextView
        android:id="@+id/tv_main_move_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:text="misolamiso"
        android:maxEms="13"
        android:singleLine="true"
        android:ellipsize="end"
        android:textColor="@color/gray9"
        android:textSize="14dp" />


    <TextView
        android:id="@+id/tv_main_move_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:lineSpacingMultiplier="1.5"
        android:text="misolamiso"
        android:textColor="@color/gray3"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/tv_main_move_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:lineSpacingMultiplier="1.5"
        android:text="misolamiso"

        android:textColor="@color/gray9"
        android:textSize="16dp"  />

    <TextView
        android:id="@+id/tv_main_move_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:paddingTop="10dp"
        android:text="misolamiso"
        android:textColor="@color/gray9"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/tv_main_move_addresse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:text="misolamiso"
        android:textColor="@color/gray9"
        android:textSize="16dp"  />

    <ImageView
        android:id="@+id/im_main_move_im"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingTop="10dp"
        android:scaleType="center"
        android:src="@mipmap/ic_launcher" />
        </LinearLayout>
</LinearLayout>
